# Run this from terminal to install rvm:
# bash < <( curl http://rvm.beginrescueend.com/releases/rvm-install-head )
# Add this line to your .profile:
# [[ -s "$HOME/.rvm/scripts/rvm" ]] && source "$HOME/.rvm/scripts/rvm"
if [[ -s "~/.rvm/environments/ruby-1.9.3@loislane" ]] ; then
  . "~/.rvm/environments/ruby-1.9.3@loislane"
else
  rvm --create use "ruby-1.9.3-p0@loislane"
fi
